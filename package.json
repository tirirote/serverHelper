{
  "name": "Serverhelper",
  "version": "1.0.0",
  "description": "Serverhelper Project launch configuration",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run dev --prefix frontend\" \"npm run start --prefix backend\"",
    "start:backend": "npm start --prefix backend",
    "start:frontend": "npm run dev --prefix frontend",
    "install":"concurrently \"npm ci --prefix frontend\" \"npm ci --prefix backend\"",
    "test": "npm run test --prefix backend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}